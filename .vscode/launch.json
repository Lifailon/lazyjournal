{
  "configurations": [
    {
      "name": "Default",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": []
    },
    {
      "name": "Custom (200K/10s)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-t", "200000", "-u", "10"]
