    {
      "name": "Disable timestamp",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-s"]
    },
    {
      "name": "Disable color",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-d"]
    },
    {
      "name": "Disable mouse",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-m"]
