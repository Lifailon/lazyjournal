{
  "configurations": [
    {
      "name": "Default",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": []
    },
    {
      "name": "Custom (200K/10s)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-t", "200000", "-u", "10"]
    },
    {
      "name": "Fast (5K/2s)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-t", "5000", "-u", "2"]
    },
    {
      "name": "Disable autoupdate",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-e"]
    },
    {
      "name": "Disable coloring",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-d"]
    },
    {
      "name": "Disable mouse control",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-m"]
    },
    {
      "name": "Docker disable timestamp",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-p"]
    },
    {
      "name": "Docker only stream",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-o"]
    },
    {
      "name": "ssh mode",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-s", "lifailon@192.168.3.101 -p 2121"]
    },
    {
      "name": "sudo ssh mode",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "args": ["-s", "lifailon@192.168.3.101 -p 2121 sudo"]
    },
  ]
}
